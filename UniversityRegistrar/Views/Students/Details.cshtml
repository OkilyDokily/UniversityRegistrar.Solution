@{
  Layout = "_Layout";
}


@using UniversityRegistrar.Models;

<p>GPA: @ViewBag.GPA</p>
<p>@Html.ActionLink("enroll in a course","SelectDepartment",new {id = @Model.StudentId})</p>
@foreach (StudentCourse join in @Model.Courses)
{
  <p>@Html.ActionLink(@join.Course.CourseName,"Details","Courses", new {id = join.Course.CourseId})</p>
}